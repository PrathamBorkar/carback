{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "functions": {
    "app.js": {
      "timeout": 30 // Set timeout to 30 seconds (maximum allowed is 60 seconds)
    }
  }
}
